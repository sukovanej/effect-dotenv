{
  "name": "effect-dotenv",
  "type": "module",
  "version": "0.4.1",
  "description": "dotenv effect wrapper",
  "homepage": "https://sukovanej.github.io/effect-dotenv",
  "types": "./index.d.ts",
  "main": "./index.js",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "require": "./index.cjs",
      "import": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "import": {
        "types": "./*.d.ts",
        "default": "./*.js"
      },
      "require": {
        "types": "./*.d.cts",
        "default": "./*.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup && cp package.json dist/ && cp README.md dist/",
    "watch": "tsc -w -p tsconfig.json",
    "format": "prettier -w src tests examples",
    "format:check": "pnpm format --check",
    "eslint": "eslint .",
    "lint": "pnpm format:check && pnpm eslint && tsc --noEmit",
    "docs-ts": "docs-ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "version": "changeset version"
  },
  "keywords": [],
  "author": "Milan Suk <Milansuk@email.cz>",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@effect/data": "^0.17.5",
    "@effect/io": "^0.38.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "peerDependencies": {
    "@effect/data": "^0.17.1",
    "@effect/io": "^0.38.0"
  }
}
